   "source": [
    "factorial_memo = {}\n",
    "\n",
    "def factorial(k):\n",
    "\n",
    "    if k < 2:\n",
    "        return 1\n",
    "\n",
    "    if not k in factorial_memo:\n",
    "        factorial_memo[k] = k * factorial(k-1)\n",
    "        print(factorial_memo)\n",
    "\n",
    "        return factorial_memo[k]\n",
    "\n"
   ]
